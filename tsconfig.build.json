{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false
  },
  "exclude": ["node_modules", "dist", "**/*.test.tsx", "**/*.stories.tsx"]
}